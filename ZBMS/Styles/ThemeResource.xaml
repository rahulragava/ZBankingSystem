                    <SolidColorBrush x:Key="SubtleText"
                                     Color="LightGray"></SolidColorBrush>
                    <SolidColorBrush x:Key="SavingsAccountCardColor"
                                     Color="CornflowerBlue" />
                    <SolidColorBrush x:Key="CurrentAccountCardColor"
                                     Color="BlueViolet"></SolidColorBrush>
                    <SolidColorBrush x:Key="PersonalLoanAccountCardColor"
                                     Color="IndianRed" />
                    <SolidColorBrush x:Key="RecurringDepositCardColor"
